{
    "name":        "cursedcoder/dark-redis-list-bundle",
    "type":        "symfony-bundle",
    "description": "Symfony2 bundle which allows you to store part of doctrine entities data in Redis.",
    "keywords":    ["redis", "list", "cursedcoder", "doctrine"],
    "homepage":    "http://github.com/cursedcoder/DarkRedisListBundle",
    "license":     "MIT",

    "authors": [
        {
            "name": "Evgeniy Guseletov",
            "homepage": "http://github.com/cursedcoder"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://github.com/cursedcoder/DarkRedisListBundle/contributors"
        }
    ],

    "require": {
        "php":                      ">=5.3.2",
        "symfony/framework-bundle": ">=2.1.0",
        "snc/redis-bundle":         "1.1.*@stable",
        "doctrine/orm":             "2.2.*@stable"
    },

    "autoload": {
        "psr-0": {
            "Dark\\RedisListBundle": "."
        }
    },

    "target-dir": "Dark/RedisListBundle"
}
